    <script>
        let dt = $('#dataTable').DataTable({
            "order": [
                [0, "desc"]
            ],
            "proccessing": true,
            "serverSide": true,
            "select": true,
            "bFilter": false,
            "bInfo": false,
            "bPaginate": false,
            "columns": [
                {
                    "data": "id",
                    "name": "id",
                    "title": "ID"
                },
                {
                    "data": "username",
                    "name": "username",
                    "title": "Username"
                },
                {
                    "data": "password",
                    "name": "password",
                    "title": "Password"
                },
                {
                    "data": "id",
                    "name": "Action",
                    "title": "Action",
                    mRender: function(data, row){
                       return `<button type="button" data-toggle="tooltip" title="Edit User!" onclick="updateUser(${data})" class="btn text-success"><i class="fa fa-edit"></i></button>` +
                        `<button id="editId" type="button" data-toggle="tooltip" title="Delete User!" onclick="deleteUser(${data})" class="btn text-danger"> <i class="fa fa-trash"></i></button>`   
                    }
                }
                
            ],
            "language": {
                "emtyTable": "No Data to show!"
            },
            "ajax": {
                "type": "get",
                "url": "/getUsers",
                "contentType": "application/json; charset=utf-8",
                "dataType": "json",
                "dataSrc": "",
            }
        });


        function deleteUser(id){
            getUser(id)
            .done((response) => {
                console.log(response);
                Swal.fire({
                title: "Are you sure Bro?",
                text: `You want to delete ${response.data.username}`,
                type: "warning",
                showDenyButton: true,
                confirmButtonColor: "#fcb03b",
                confirmButtonText: "Yes, delete it",
                denyButtonText: "No bitch!!",
                closeOnConfirm: false,
                closeOnCancel: false,
                }).then((result) => {
                    window.result = result;
                    console.log(window.result);
                    if(result.isConfirmed){
                        $.ajax({
                            type: 'DELETE',
                            url: '/deleteUser',
                            data: {
                                id: id
                            },
                            dataType: 'json',
                            success: function(result){
                                $.toast({
                                    heading: "Success",
                                    text: "Record Deleted Successfully",
                                    position: 'top-right',
                                    loaderBg: "#effff8",
                                    icon: "success",
                                    showHideTransition: 'plain',
                                    allowToastClose: true,
                                    hideAfter: 3500
                                });
                                dt.ajax.reload();
                                dt.draw();
                                if(result.status == 200){
                                    Swal.fire('Deleted!', '', 'success')
                                }
                                if(result.status == 500){
                                    Swal.fire('Internal Server Error!', '', 'error')
                                    console.log("Internal Server Error");        
                                }             
                            },
                            error: function(err){
                                console.log("Cancelled due to: ", err);
                            }
                        });
                    } else if (result.isDenied) {
                        Swal.fire("Cancelled", "Your selected record is safe :", "info");
                    }
                });             

            })
            .fail((err) => {
                console.log(err);
            });
            
        }

        function getUser(id){
            return $.ajax({
                type: 'GET',
                url: '/getUser?id='+id,
                dataType: 'json'
            });
        }


        function updateUser(id){
            $.ajax({
                url: "/getUser?id="+id,
                method: 'get',
                dataType: "json",
                success: (result) => {
                    if(result.status == 200){
                        Swal.fire({
                            title: `Edit User ${result.data.username}`,
                            text: 'Modal with a custom image.',
                            imageUrl: '/img/undraw_profile_1.svg',
                            imageWidth: 300,
                            imageHeight: 200,
                            imageAlt: 'Custom image',
                            html: '<input type="text" id="swal-input1" class="swal2-input" placeholder="Username">' +
                                    '<input type="password" id="swal-input2" class="swal2-input" placeholder="Password">' +
                                    '<input type="password" id="swal-input3" class="swal2-input" placeholder="Confirm Password">',
                            preConfirm: function () {
                                const username = Swal.getPopup().querySelector('#swal-input1').value;
                                const password = Swal.getPopup().querySelector('#swal-input2').value;
                                const confirmPassword = Swal.getPopup().querySelector('#swal-input3').value;
                                if (password === ""|| username === "" || confirmPassword === "") {
                                    Swal.showValidationMessage(`Fields cannot be empty!`);
                                }
                                if (password !== confirmPassword) {
                                    Swal.showValidationMessage(`Password and confirmPassword are different!`);
                                }
                                return { username: username, password: password }
                            },
                        }).then((res) => {
                            if (res.value) {
                                console.log("Result: " + JSON.stringify(res));

                                $.ajax({
                                    url: '/updateUser',
                                    method: "POST",
                                    dataType: 'json',
                                    data: {
                                        id: id,
                                        username: res.value.username,
                                        password: res.value.password
                                    },
                                    success: (result) => {
                                        console.log(result);
                                        if(result.status == 200){
                                            Swal.fire({
                                                icon: 'success',
                                                title: 'User Updated',
                                                text: `User's new username is ${res.value.username}`,
                                            });
                                            dt.ajax.reload();
                                            dt.draw();
                                        }
                                    },
                                    error: (err) => {
                                        console.log(err);
                                    }
                                });
                            }
                        });
                    }
                    if(result.status == 500){
                        console.log("Internal Server error")
                    }
                },
                error: (err) => {
                    console.log("Internal Server error", err)
                }
            })
        }

    </script>
